                        %li
                          %a{:href => "archive.html"} Sub Category 2.3
                        %li
                          %a{:href => "archive.html"} Sub Category 2.4
                    .sf-mega-posts
                      .hentry.post
                        %a{:href => "single_post.html", :rel => "bookmark"}
                          %img.entry-thumbnail{:alt => "Post Title", :src => ""}/
                        %h2.entry-title
                          %a{:href => "single_post.html", :rel => "bookmark"} Vestibulum Quam Neque Pretium Eu Consectetur Quis
                      .hentry.post
                        %a{:href => "single_post.html", :rel => "bookmark"}
                          %img.entry-thumbnail{:alt => "Post Title", :src => ""}/
                        %h2.entry-title
                          %a{:href => "single_post.html", :rel => "bookmark"} Ipsum Hendrerit Sed Neque Sed Sapien Rutrum Laoreet
                      .hentry.post
                        %a{:href => "single_post.html", :rel => "bookmark"}
                          %img.entry-thumbnail{:alt => "Post Title", :src => ""}/
                        %h2.entry-title
                          %a{:href => "single_post.html", :rel => "bookmark"} Congue Sed Ligula Nam Dolor Ligula Faucibus Id Sodales
                    / .sf-mega-posts
                  / .sf-mega
                %li
                  %a{:href => "archive.html"} Category 3
                %li
                  %a{:href => "archive.html"} Category 4
                %li
                  %a{:href => "archive.html"} Dropdown
                  %ul
                    %li
                      %a{:href => "archive.html"} Sub Menu 1
                      %ul
                        %li
                          %a{:href => "archive.html"} Sub Menu 1.1
                          %ul
                            %li
                              %a{:href => "archive.html"} Sub Menu 1.1.1
                            %li
                              %a{:href => "archive.html"} Sub Menu 1.1.2
                            %li
                              %a{:href => "archive.html"} Sub Menu 1.1.3
                        %li
                          %a{:href => "archive.html"} Sub Menu 1.2
                        %li
