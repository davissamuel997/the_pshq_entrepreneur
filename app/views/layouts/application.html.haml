                    %li
                      %a{:href => "archive.html"} Sub Menu 2
                    %li
                      %a{:href => "archive.html"} Sub Menu 3
                %li.pull-right
                  - if !current_user
                    %a{type: "button", 'data-toggle' => 'modal', href: "#send-recording"}
                      Login
                      %i.fa.icon-lock
            / Secondary Navigation / End
            .header-social-icons
              %span Follow:
              %a.tooltip{:href => "#", :title => "Facebook"}
                %i.fa.fa-facebook
              %a.tooltip{:href => "#", :title => "Twitter"}
                %i.fa.fa-twitter
              %a.tooltip{:href => "#", :title => "GooglePlus"}
                %i.fa.fa-google-plus
              %a.tooltip{:href => "#", :title => "Pinterest"}
                %i.fa.fa-pinterest
              %a.tooltip{:href => "#", :title => "LinkedIn"}
                %i.fa.fa-linkedin
              %a.tooltip{:href => "#", :title => "RSS"}
                %i.fa.fa-rss
              %a.tooltip{:href => "#", :title => "Newsletter"}
                %i.fa.fa-envelope-o
