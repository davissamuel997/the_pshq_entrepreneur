%html{:lang => "en", 'ng-app' => 'ThePshqEntrepreneur'}
  / <![endif]
  %head
    / Meta
    %base{:href => "/"}
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %meta{:content => "width=device-width, initial-scale=1, maximum-scale=1", :name => "viewport"}
      %meta{:content => "TheWorld is a fresh and clean magazine website template which is beautiful and well-organized", :name => "description"}
        %meta{:content => "magazine template, news template", :name => "keywords"}
          %meta{:content => "themejunkie", :name => "author"}
            / Title
            %title ThePSHQEntrepreneur

            = stylesheet_link_tag "jquery-ui-1.11.2.custom.css"
            = stylesheet_link_tag 'gritter/jquery.gritter.css'
            = stylesheet_link_tag "chosen.css"
            = stylesheet_link_tag "theme/world-theme-include.css"
            = stylesheet_link_tag "theme2/theme2.css"

  %body

    / Page / Start
    #page.hfeed.site.clearfix
      / Site Main / Start
      %main#main.site-main{:role => "main"}

        %header#masthead.site-header.clearfix{:role => "banner"}
          .container
            #logo.no-padding
              %a{:href => "/"}
                = image_tag "PSHQ-logo.jpg"

            / Primary Navigation / Start
            %nav#primary-nav
              %ul#primary-menu.sf-menu
                %li
                  %a{:href => "single_page.html"}
                    PSHQ
                    %span.menu-desc Learn About the Business
                  .sf-mega.cols-3
                    .sf-mega-section
                      %h3 Page Layouts
                      %ul
                        %li
                          %a{:href => "archive.html"} Archive
                        %li
                          %a{:href => "single_page.html"} Single Page
                        %li
                          %a{:href => "single_post.html"} Single Post
                        %li
                          %a{:href => "search.html"} Search
                    .sf-mega-section
                      %h3 Other Pages
                      %ul
                        %li
                          %a{:href => "404.html"} 404 Page
                        %li
                          %a{:href => "full_width.html"} Full Width
                        %li
                          %a{:href => "left_sidebar.html"} Left Sidebar
                        %li
                          %a{:href => "right_sidebar.html"} Right Sidebar
                    .sf-mega-section.last-section
                      %h3 Paragraph
                      %p
                        This Mega Menu can handle everything. Lists, paragraphs, forms...
                  / .sf-mega
                %li
                  %a{:href => "about.html"}
                    About
                    %span.menu-desc Meet Your Hosts
                %li
                  %a{:href => "contact.html"}
                    Contact
                    %span.menu-desc Get In Touch
                %li
                  %a{:href => "single_page.html"}
                    Dropdown
                    %span.menu-desc With Sub-menus
                  %ul
                    %li
                      %a{:href => "single_page.html"} Sub Menu 1
                      %ul
                        %li
                          %a{:href => "single_page.html"} Sub Menu 1.1
                          %ul
                            %li
                              %a{:href => "single_page.html"} Sub Menu 1.1.1
                            %li
                              %a{:href => "single_page.html"} Sub Menu 1.1.2
                            %li
                              %a{:href => "single_page.html"} Sub Menu 1.1.3
                        %li
                          %a{:href => "single_page.html"} Sub Menu 1.2
                        %li
                          %a{:href => "single_page.html"} Sub Menu 1.3
                    %li
                      %a{:href => "single_page.html"} Sub Menu 2
                    %li
                      %a{:href => "single_page.html"} Sub Menu 3
                %li
                  %a{:href => "typography.html"}
                    Typography
                    %span.menu-desc Element Styles
            / Primary Navigation / End
            .clearfix
        / Masthead / End
        / Secondary Bar / Start
        #secondary-bar
          .container.clearfix
            %a#secondary-mobile-menu{:href => "#"}
              %i.fa.fa-bars
              %span Secondary Menu
            / Secondary Navigation / Start
            %nav#secondary-nav
              %ul#secondary-menu.sf-menu
                %li.current_item
                  %a{:href => "index.html"} Home
                %li
                  %a{:href => "archive.html"} Category 1
                  .sf-mega.cols-4
                    .sf-mega-section
                      %ul
                        %li
