
#primary.content-area
  / Featured Content / Start
  #featured-content.clearfix.no-padding
    .container

      %carousel{"no-wrap" => "noWrapSlides"}
        %slide{:active => "podcast.active", "ng-repeat" => "podcast in requestControl.podcasts"}

          %ul
            %li
              %article.hentry.clearfix
                %a{:href => '/podcasts/{{podcast.podcast_id}}', :rel => 'bookmark'}
                  %img.entry-thumbnail{'ng-src' => "{{podcast.image}}", :style => "margin:auto;"}
                .entry-meta
                  %span.entry-date {{podcast.air_date}}
                %p
                  %h3.entry-title{:style => 'margin: 0; height: 25px;'}
                    %span.entry-date{:style => 'color: white;'} Episode: {{podcast.episode_number}}
                %p
                  %h2.entry-title{:style => 'margin: 0;'}
                    %span.entry-date{:style => 'color: white;'} {{podcast.name}}
                .entry-summary
                  {{podcast.summary}}
                .entry-share-icons
                  %a{:href => "#", :title => "Twitter"}
                    %i.fa.fa-twitter
                  %a{:href => "#", :title => "Facebook"}
                    %i.fa.fa-facebook
                  %a{:href => "#", :title => "GooglePlus"}
                    %i.fa.fa-google-plus
                  %a{:href => "#", :title => "Pinterest"}
                    %i.fa.fa-pinterest
                  %a{:href => "#", :title => "LinkedIn"}
                    %i.fa.fa-linkedin

    / END list group
  .panel-footer
    .clearfix
      %article.hentry.post.clearfix
        .padding-md.clearfix
          .panel.panel-default.table-responsive
            .panel-heading
              %h1.entry-title.text-center Are you the 2015 Entrepreneur?
            
        %a{:href => "single_post.html", :rel => "bookmark"}
          = image_tag "jake_davis-100-100.jpg", :class => 'entry-thumbnail'
        .byline
          %span.entry-view.make-pshq-green Jake Davis

        .entry-share-icons
          %a{:href => "#", :title => "Twitter"}
            %i.fa.fa-twitter
          %a{:href => "#", :title => "Facebook"}
            %i.fa.fa-facebook
          %a{:href => "#", :title => "GooglePlus"}
            %i.fa.fa-google-plus
          %a{:href => "#", :title => "Pinterest"}
            %i.fa.fa-pinterest
          %a{:href => "#", :title => "LinkedIn"}
            %i.fa.fa-linkedin

  .container
    / More Content / Start
    #more-content.content-loop
      %h3.section-title
        Latest News
        %span.see-all
          %a{:href => "/posts"}
            Browse All News
            %i.fa.fa-angle-right
      %ul
        %li.clearfix{'ng-repeat' => 'post in requestControl.posts'}
          %article.hentry.post.clearfix
            %a{:href => '/posts/{{post.post_id}}'}
              %img.entry-thumbnail{:alt => "Post Title", :src => ""}/
            .entry-meta
              %span.entry-category
                %a{:href => "archive.html"} Category 1
              %span.entry-date {{post.post_date}}
            %h2.entry-title
              %a{:href => '/posts/{{post.post_id}}', :rel => 'bookmark'} {{post.name}}
            .byline
              %span.entry-view 89 Views
              %span.entry-comment
                %a{:href => "single_post.html#comments"} 14
              %span.entry-author
                by
                %a{:href => "archive.html"} {{post.user.full_name}}
            .entry-summary
              {{post.summary}}
            .entry-share-icons
              %a{:href => "#", :title => "Twitter"}
                %i.fa.fa-twitter
              %a{:href => "#", :title => "Facebook"}
                %i.fa.fa-facebook
              %a{:href => "#", :title => "GooglePlus"}
                %i.fa.fa-google-plus
              %a{:href => "#", :title => "Pinterest"}
                %i.fa.fa-pinterest
              %a{:href => "#", :title => "LinkedIn"}
                %i.fa.fa-linkedin


