    / More Content / Start
    #more-content
      %article.hentry.post
        %h1.page-title {{requestControl.podcast.name}}
        %a.btn.btn-sm.btn-danger{:href => '/podcasts/{{requestControl.podcast.podcast_id}}/edit'} Edit Podcast
        .entry-content
        
          %h4 Here it is
          %button.btn.btn-sm.btn-success{"ng-click" => "audio.paused ? audio.play() : audio.pause()"} Pause
          %button{"ng-click" => "audio.restart()"} Stop
          %button{"ng-click" => "audio.muting = !audio.muting"} Mute
          %label Current Time {{audio.currentTime | trackTime}}
          %input.form-control{"ng-model" => "audio.currentTime", :type => "text"}
            %label Volume
            %input.form-control{:max => "1", :min => "0", "ng-model" => "audio.volume", :step => "0.01", :type => "range"}
          .input-group
            %span.input-group-addon Progress
            %input.form-control{:max => "1", :min => "0", "ng-model" => "audio.progress", :step => "0.01", :type => "range"}

          .input-group
            %span.input-group-addon Progress
            #bar.progress.active
              .progress-bar.progress-bar-success.progress-bar-striped.active{"aria-valuemax" => "100", "aria-valuemin" => "0", "aria-valuenow" => "60", :role => "progressbar", 'ng-style' => "{'width': requestControl.getProgressBarWidth()}"}

          {{requestControl.getProgressBarWidth()}}

          %table.table.table-striped.table-bordered.table-condensed
            %tr
              %td
                src
              %td
                {{audio.src}}
            %tr
              %td
                Current Time
              %td
                {{audio.currentTime | number : 2}}
            %tr
              %td Duration
              %td {{audio.duration | number : 2}}
            %tr
              %td Progress (%)
              %td {{audio.progress | number : 1}}
            %tr
              %td Volume
              %td {{audio.volume | number : 1}}

